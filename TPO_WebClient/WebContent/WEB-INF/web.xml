<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" 
xmlns="http://java.sun.com/xml/ns/j2ee" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>TPO_WebClient</display-name>
	<welcome-file-list>
    <welcome-file>home.jsp</welcome-file>
  	</welcome-file-list>
  	<servlet>
	    <description></description>
	    <display-name>home</display-name>
	    <servlet-name>home</servlet-name>
	    <servlet-class>login.home</servlet-class>
  	</servlet>
	<servlet-mapping>
		<servlet-name>home</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	
	<servlet>
	    <description></description>
	    <display-name>login</display-name>
	    <servlet-name>login</servlet-name>
	    <servlet-class>login.login</servlet-class>
  	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>	
	
	<servlet>
	    <description></description>
	    <display-name>abmEnvios</display-name>
	    <servlet-name>abmEnvios</servlet-name>
	    <servlet-class>abm.abmEnvios</servlet-class>
  	</servlet>
	<servlet-mapping>
		<servlet-name>abmEnvios</servlet-name>
		<url-pattern>/abmEnvios</url-pattern>
	</servlet-mapping>	
</web-app>
	